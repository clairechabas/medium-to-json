{
  "name": "rss-to-json-parser",
  "version": "1.0.0",
  "description": "A simple util to turn my Medium post RSS feed into JSON.",
  "main": "index.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\""
  },
  "keywords": [],
  "author": "Claire Chabas",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.5",
    "express": "^4.18.2",
    "rss-to-json": "^2.1.1",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.21"
  }
}
